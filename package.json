{
    "name": "generator-fp-ts",
    "version": "0.0.0",
    "description": "Yeoman generator for fp-ts projects.",
    "homepage": "https://github.com/mrbandler/generator-fp-ts",
    "author": {
        "name": "mrbandler",
        "email": "me@mrbandler.dev",
        "url": "https://mrbandler.dev"
    },
    "files": [
        "generators"
    ],
    "main": "generators/index.js",
    "keywords": [
        "fp-ts",
        "yeoman-generator"
    ],
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@types/jest": "^26.0.19",
        "@types/lodash": "^4.14.167",
        "@types/yeoman-assert": "^3.1.1",
        "@types/yeoman-generator": "^4.11.3",
        "@types/yeoman-test": "^2.0.5",
        "@typescript-eslint/eslint-plugin": "^4.11.1",
        "@typescript-eslint/parser": "^4.11.1",
        "eslint": "^7.17.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-config-xo": "^0.27.2",
        "eslint-plugin-prettier": "^3.3.0",
        "husky": "^3.0.9",
        "jest": "^26.6.3",
        "lint-staged": "^9.4.3",
        "prettier": "^2.2.1",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3",
        "yeoman-assert": "^3.1.0",
        "yeoman-test": "^1.7.0"
    },
    "engines": {
        "npm": ">= 4.0.0"
    },
    "dependencies": {
        "@types/chalk": "^2.2.0",
        "@types/yosay": "^0.0.29",
        "chalk": "^2.1.0",
        "lodash": "^4.17.20",
        "yeoman-generator": "^2.0.1",
        "yosay": "^2.0.1"
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "scripts": {
        "build": "tsc",
        "test": "tsc && jest"
    },
    "repository": "git@github.com:mrbandler/generator-fp-ts.git",
    "license": "MIT"
}
